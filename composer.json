{
  "name": "valesaude/telemedicine-client",
  "type": "library",
  "autoload": {
    "psr-4": {
      "ValeSaude\\TelemedicineClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ValeSaude\\TelemedicineClient\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Vale Sa√∫de Sempre",
      "email": "ti@valesaudesempre.com.br"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "illuminate/contracts": "^8.83",
    "illuminate/support": "^8.83",
    "spatie/laravel-package-tools": "^1.12",
    "valesaude/laravel-value-objects": "^1.0.0",
    "illuminate/http": "*",
    "guzzlehttp/guzzle": "^7.5",
    "nesbot/carbon": "^2.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9",
    "pestphp/pest": "^1.22",
    "pestphp/pest-plugin-laravel": "^1.3",
    "phpstan/extension-installer": "^1.2",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.2",
    "fakerphp/faker": "^1.20",
    "orchestra/testbench": "^6.25"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ValeSaude\\TelemedicineClient\\TelemedicineClientServiceProvider"
      ]
    }
  },
  "scripts": {
    "analyse": "phpstan analyze",
    "test": "pest",
    "test:coverage": "pest --coverage"
  }
}
